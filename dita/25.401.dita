               <colspec colnum="3" colname="3" colwidth="8*" rowheader="headers"/>
               <colspec colnum="4" colname="4" colwidth="12*" rowheader="headers"/>
               <colspec colnum="5" colname="5" colwidth="14*" rowheader="headers"/>
